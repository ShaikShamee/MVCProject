
@model StudentDetails.Models.OrgnazatioRepo
@{
    ViewBag.Title = "Home Page";
}
<h2>@ViewBag.Message</h2>

<p><b>Teacher List</b></p>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>Email</th>
        <th>PhoneNumber</th>
        <th>Age</th>
    </tr>
    @foreach (ITEmployee itemployees in ViewBag.orgnazations)
    {
        <tr>
            <td>@itemployees.EmpId</td>
            <td>@itemployees.EmpName</td>
            <td>@itemployees.Email</td>
            <td>@itemployees.PhoneNumber</td>
            <td>@itemployees.Age</td>
        </tr>
    }
</table>

